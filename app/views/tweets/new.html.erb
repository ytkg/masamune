<% content_for :page_title, 'ツイート（開発中）' %>
<div class="col-lg-12">
  <div class="card">
    <div class="card-body">
      <%= form_with url: tweets_path do |form| %>
        <p>
          <%= form.text_area :tweet_text, class: 'form-control', disabled: true %>
        </p>
        <p class="text-right mb-0"><%= form.submit 'ツイート', class: 'btn btn-info', disabled: true %></p>
      <% end %>
    </div>
  </div>
</div>
